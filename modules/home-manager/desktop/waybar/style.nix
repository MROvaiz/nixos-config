{colorscheme}:
with colorscheme.colors; ''
  /* CREATED BY KITTENZEXE */

  * {
    border: none;
    border-radius: 0;
    min-height: 24px;
    font-family: "JetBrainsMono Nerd Font Mono";
    font-weight: 500;
    font-size: 12px;
    padding: 0;
  }

  window#waybar {
    background: transparent;
  }

  tooltip {
    border-radius: 12px;
    background-color: #121B35;
  }

  #clock {
    background-color: #121B35;
    color: #3a688c;
    font-size: 12px;
    padding-left: 10px;
    padding-right: 10px;
    border-radius: 0px 0px 12px 12px;
  }

  #workspaces {
    background-color: #121B35;
    color: #6096a3;
    padding-right: 10px;
    border-radius: 0px 0px 12px 0;
  }

  #workspaces button.visible {
    color: #3a688c;
  }

  #workspaces button {
    margin: 0px 5px;
    padding: 0 5px;
    border-radius: 6px;
  }

  #battery, #tray {
    background-color: #121B35;
    color: #3a688c;
    font-size: 12px;
    margin-right: 0px;
    padding-left: 10px;
    padding-right: 10px;
  }

  #pulseaudio {
    background-color: #121B35;
    color: #3a688c;
    font-size: 12px;
    padding-left: 10px;
    padding-right: 10px;
    border-radius: 0px 0px 12px 0;
  }

  #cpu, #temperature {
    background-color: #121B35;
    color: #3a688c;
    font-size: 12px;
    padding-left: 10px;
    padding-right: 10px;
  }

  #memory {
    background-color: #121B35;
    color: #3a688c;
    font-size: 12px;
    padding-left: 10px;
    padding-right: 10px;
  }

  #network {
    background-color: #121B35;
    color: #3a688c;
    font-size: 12px;
    padding-left: 10px;
    padding-right: 10px;
    border-radius: 0px 0px 0px 12px;
  }

  #image {
    background-color: #121B35;
    padding-left: 10px;
    padding-right: 10px;
    border-radius: 0px 0px 0px 12px;
  }

  #custom-quit, #custom-lock, #custom-reboot, #custom-power {
    background-color: #121B35;
    color: #3a688c;
    font-size: 18px;
    padding-left: 10px;
    padding-right: 10px;
  }

  #custom-icon-arch, #custom-logo {
    background-color: #121B35;
    color: #3a688c;
    font-size: 18px;
    padding-left: 8px;
    padding-right: 8px;
    border-radius: 0px 0px 0px 12px;
  }

  #custom-icon-left {
    color: #121B35;
    font-size: 34px;
    margin-top: -24px;
    margin-right: -14px;
  }

  #custom-icon-left-outer {
    color: #121B35;
    font-size: 34px;
    margin-top: -24px;
    margin-right: -14px;
    margin-left: 10px;
  }

  #custom-icon-split {
    background-color: #121B35;
    color: #3a688c;
    font-size: 12px;
  }

  #custom-icon-right {
    color: #121B35;
    font-size: 34px;
    margin-top: -24px;
    margin-left: -13px;
  }

  #custom-icon-right-outer {
    color: #121B35;
    font-size: 34px;
    margin-top: -24px;
    margin-left: -13px;
    margin-right: 10px;
  }
''
